<h1>Grandma Enjoys Musics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Form</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grandma_enjoys_musics.each do |grandma_enjoys_music| %>
      <tr>
        <td><%= grandma_enjoys_music.name %></td>
        <td><%= grandma_enjoys_music.form %></td>
        <td><%= link_to 'Show', grandma_enjoys_music %></td>
        <td><%= link_to 'Edit', edit_grandma_enjoys_music_path(grandma_enjoys_music) %></td>
        <td><%= link_to 'Destroy', grandma_enjoys_music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

